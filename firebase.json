{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/landing.html"
      },
      {
        "source": "/app",
        "destination": "/app.html"
      },
      {
        "source": "**",
        "destination": "/landing.html"
      }
    ]
  }
}
